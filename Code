       public static List<List<Dictionary<string, object>>> GetUploaderIndiaValidData(
        string componentGroupId,
        string projectID,
        int page,
        int displayLength,
        string orderBy,
        int sortOrder,
        int getPages)
        {
            List<List<Dictionary<string, object>>> rowsList = new List<List<Dictionary<string, object>>>();
            List<Dictionary<string, object>> rows = new List<Dictionary<string, object>>();
            try
            {
                string strRoleId = HttpContext.Current.Session["RoleId"].ToString();
                int RoleId = Convert.ToInt32(strRoleId);

                ComponentDetails objCompDetails = new ComponentDetails()
                {
                    COMPONENTGROUPID = Convert.ToInt64(componentGroupId),
                    ProjectId = projectID
                };
                objCompDetails.PageNum = page;
                objCompDetails.IDisplayLength = displayLength;
                objCompDetails.OrderBy = orderBy;
                objCompDetails.SortOrder = sortOrder;
                objCompDetails.RoleID = RoleId;
                objCompDetails.CountryID = Convert.ToInt32(HttpContext.Current.Session["CountryID"].ToString());
                if (getPages == 1)
                {
                    objCompDetails.GetPages = getPages;
                }

                DataSet gridUserDetails = new DataSet();

                if (componentGroupId.Equals("1") || componentGroupId.Equals("2") || componentGroupId.Equals("3") ||
                    componentGroupId.Equals("4") || componentGroupId.Equals("5") || componentGroupId.Equals("6") ||
                    componentGroupId.Equals("7") || componentGroupId.Equals("8") || componentGroupId.Equals("9") ||
                    componentGroupId.Equals("10") || componentGroupId.Equals("11") || componentGroupId.Equals("12") ||
                    componentGroupId.Equals("16") || componentGroupId.Equals("15") || componentGroupId.Equals("41") ||
                    componentGroupId.Equals("43") || componentGroupId.Equals("44") || componentGroupId.Equals("98") ||
                    componentGroupId.Equals("100") || componentGroupId.Equals("102") || componentGroupId.Equals("103") ||
                    componentGroupId.Equals("104")|| componentGroupId.Equals("105"))
                {
                    objCompDetails.LoginId = HttpContext.Current.Session["UserId"].ToString();
                    gridUserDetails = UtilityBAL.GetUploaderValidData(objCompDetails);
                }


                rows = null;
                if (gridUserDetails.Tables.Count > 0 && gridUserDetails.Tables[0].Rows.Count > 0)
                {
                    DataTable gridUserDetailsTable = gridUserDetails.Tables[0];
                    Dictionary<string, object> row = null;
                    rows = new List<Dictionary<string, object>>();
                    foreach (DataRow dr in gridUserDetailsTable.Rows)
                    {
                        row = new Dictionary<string, object>();
                        foreach (DataColumn col in gridUserDetailsTable.Columns)
                        {
                            row.Add(col.ColumnName, dr[col]);
                        }

                        rows.Add(row);
                    }

                    rowsList.Add(rows);
                }
                else
                {
                    rows = new List<Dictionary<string, object>>();
                    rowsList.Add(rows);
                }

                rows = null;
                if (gridUserDetails.Tables.Count > 1)
                {
                    if (gridUserDetails.Tables[1].Rows.Count > 0)
                    {

                        DataTable gridUserDetailsTable = gridUserDetails.Tables[1];
                        Dictionary<string, object> row = null;
                        rows = new List<Dictionary<string, object>>();
                        foreach (DataRow dr in gridUserDetailsTable.Rows)
                        {
                            row = new Dictionary<string, object>();
                            foreach (DataColumn col in gridUserDetailsTable.Columns)
                            {
                                row.Add(col.ColumnName, dr[col]);
                            }

                            rows.Add(row);
                        }

                        rowsList.Add(rows);
                    }
                }
            }
            catch (SystemException ex)
            {
                ErrorLogger logger = new ErrorLogger();
                logger.LogError(ex);
            }

            return rowsList;
        }

        #endregion GetUploaderIndiaValidData
