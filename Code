		INNER JOIN ComponentMaster CM WITH(NOLOCK) ON STP.Fk_ComponentID=CM.PK_ComponentId
		INNER JOIN #MINDATE MD WITH(NOLOCK) ON MD.ComponentID=STP.Fk_ComponentID AND MD.ProjectID=STP.ProjectID
