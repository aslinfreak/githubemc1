/// <summary>
/// Method to update Batch Job Details
/// </summary>
/// <param name="batchJobDetails">the data to update</param>
/// <returns>status of the operation</returns>
public string UpdateBatchJobDetails(BatchJobData batchJobDetails)
{
    //// To update Batch Job Details
    try
    {
        using (AdminPageDL objDl = new AdminPageDL())
        {
            return objDl.UpdateBatchJobDetails(batchJobDetails);
        }
    }
    catch (SystemException ex)
    {
        throw new SystemException("UpdateBatchJobDetails", ex);
    }
}
