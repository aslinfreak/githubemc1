function opentemplate() {

    var keycode = '';    
    keycode = 'Bulk Approval';

    var datastring = '{"keycode":"' + keycode + '","componentGroupID":0}';
    $.ajax({
        type: "POST",
        url: "WebMethods.aspx/GetEmptyTemplateURL",
        contentType: "application/json; charset=utf-8",
        data: datastring,
        dataType: "json",
        cache: false,
        success: function (msg) {
            if (msg.d != '') {
                const url = msg.d;
                $("#progressBackgroundFilter, #processMessage").show();

                document.cookie = "fileDownload=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
                const checkInterval = setInterval(() => {
                    if (document.cookie.includes("fileDownload=true")) {
                        clearInterval(checkInterval);
                        $("#progressBackgroundFilter, #processMessage").hide();
                    }
                }, 1000);
                window.location.href = url;
            }
            else {
                errorpopup('No Template to download');
            }
        },
        error: function (xhr) {
            errorpopup(parent.document.getElementById("hdnSession").value);
        }
    });
}
